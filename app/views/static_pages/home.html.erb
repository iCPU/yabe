  <% if signed_in? %>
    <div class="row">
      <aside class="span4">
        <section>
	  <h3>New Search</h3>
	  <br/>
          <%= render 'shared/search_form' %>
        </section>
        
        <section>
          <h3>Category Finder</h3>
          <br/>
          <%= render 'shared/cat_search' %>
        </section>
      </aside>
      <div class="span8">
        <h3>Recent Searches</h3>
        <%= render 'shared/feed' %>
      </div>
    </div>
  <% else %>
    <%= render 'shared/signin_form' %>
  <% end %> 
