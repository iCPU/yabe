  <% if signed_in? %>
    <div class="row">
      <aside class="span4">
        <section>
	  <h3>New Search</h3>
	  <br/>
          <%= render 'shared/search_form' %>

        </section>
      </aside>
      <div class="span8">
        <h3>Recent Searches</h3>
        <%= render 'shared/feed' %>
      </div>
    </div>
    <%= render 'shared/cat_search' %>  
 
  <% for category in Category.roots %>
    <li><%= category.ebay_cat_name %></li>
  <% end %>

  <% else %>
    <%= render 'shared/signin_form' %>
  <% end %> 
